#!/bin/sh

. /lib/functions/uci-defaults.sh

board_config_update

board=$(cat /tmp/sysinfo/board_name)

case $board in
  dir-815-d1)
	ucidef_set_led_switch "lan1" "lan1" "rtl819x:green:lan1" "switch0" "0x1"
	ucidef_set_led_switch "lan2" "lan2" "rtl819x:green:lan2" "switch0" "0x2"
	ucidef_set_led_switch "lan3" "lan3" "rtl819x:green:lan3" "switch0" "0x4"
	ucidef_set_led_switch "lan4" "lan4" "rtl819x:green:lan4" "switch0" "0x8"
	ucidef_set_led_switch "wan" "wan" "rtl819x:green:wan" "switch0" "0x10"
	ucidef_set_led_netdev "wifi_led_2g" "wifi_2g" "rtl8192cd:green:wifi2" "wlan0"
	ucidef_set_led_netdev "wifi_led_5g" "wifi_5g" "rtl8192cd:green:wifi5" "wlan1"
	;;
  gwr-300n-v1|\
  re172-v1)
	ucidef_set_led_switch "lan1" "lan1" "rtl819x:green:lan1" "switch0" "0x1"
	ucidef_set_led_switch "lan2" "lan2" "rtl819x:green:lan2" "switch0" "0x2"
	ucidef_set_led_switch "lan3" "lan3" "rtl819x:green:lan3" "switch0" "0x4"
	ucidef_set_led_switch "lan4" "lan4" "rtl819x:green:lan4" "switch0" "0x8"
	ucidef_set_led_switch "wan" "wan" "rtl819x:green:wan" "switch0" "0x10"
	ucidef_set_led_netdev "wifi_led_2g" "wifi_2g" "rtl8192cd:green:wifi2" "wlan0"
	;;
  actionrg1200-v1)
	ucidef_set_led_netdev "lan" "lan" "actionrg1200:blue:lan" "eth0.1" "link"
	ucidef_set_led_netdev "wan" "wan" "actionrg1200:blue:wan" "eth0.2" "link"
	ucidef_set_led_netdev "wifi0" "wifi0" "actionrg1200:blue:wifi0" "wlan0" "link"
	ucidef_set_led_default "sys" "sys" "actionrg1200:blue:sys" "1"
	;;
  actionrf1200-v1)
	ucidef_set_led_netdev "wifi0" "wifi0" "actionrf1200:blue:wifi0" "wlan0" "link"
	ucidef_set_led_default "sys" "sys" "actionrf1200:blue:sys" "1"
	ucidef_set_led_netdev "wan" "wan" "actionrf1200:blue:wan" "eth0.2" "link"
	ucidef_set_led_switch "lan1" "lan1" "actionrf1200:blue:lan0" "switch0" "0x2"
	ucidef_set_led_switch "lan2" "lan2" "actionrf1200:blue:lan1" "switch0" "0x4"
	ucidef_set_led_switch "lan3" "lan3" "actionrf1200:blue:lan2" "switch0" "0x8"
	;;
  gf1200-v1)
	ucidef_set_led_switch "lan" "lan" "gf1200:blue:lan" "switch0" "0x1e"
	ucidef_set_led_netdev "wan" "wan" "gf1200:blue:wan" "eth0.2" "link"
	ucidef_set_led_netdev "multiwlan" "multiwlan" "gf1200:blue:wlan" "wlan1" "link"
	;;
  w5-1200f-v1)
	ucidef_set_led_switch "lan" "lan" "w51200f:green:lan" "switch0" "0x1e"
	ucidef_set_led_netdev "wan" "wan" "w51200f:green:wan" "eth0.2" "link"
	ucidef_set_led_netdev "wlan2g" "wlan2g" "w51200f:green:wlan2g" "wlan0" "link"
	ucidef_set_led_netdev "wlan5g" "wlan5g" "w51200f:green:wlan5g" "wlan1" "link"
	;;
  re708-v1)
	ucidef_set_led_switch "wan" "wan" "rtl8367r:green:wan" "switch0" "0x10"
	ucidef_set_led_switch "lan1" "lan1" "rtl8367r:green:lan1" "switch0" "0x1"
	ucidef_set_led_switch "lan2" "lan2" "rtl8367r:green:lan2" "switch0" "0x2"
	ucidef_set_led_switch "lan3" "lan3" "rtl8367r:green:lan3" "switch0" "0x4"
	ucidef_set_led_switch "lan4" "lan4" "rtl8367r:green:lan4" "switch0" "0x8"
	ucidef_set_led_netdev "wifi0" "wifi0" "re708:green:wlan_2g" "wlan0" "link"
	ucidef_set_led_netdev "wifi1" "wifi1" "re708:green:wlan_5g" "wlan1" "link"
	ucidef_set_led_default "wps" "wps" "re708:green:wps" "0"
	;;
  gwr1200ac-v1|\
  gwr1200ac-v2)
	ucidef_set_led_netdev "wifi0" "wifi0" "gwr1200ac:green:wlan_2g" "wlan0" "link"
	ucidef_set_led_netdev "wifi1" "wifi1" "gwr1200ac:green:wlan_5g" "wlan1" "link"
	ucidef_set_led_default "wps" "wps" "gwr1200ac:green:wps" "0"
	;;
  maxlinkac1200g-v1)
	ucidef_set_led_netdev "wifi0" "wifi0" "maxlinkac1200g:green:wlan_2g" "wlan0" "link"
	ucidef_set_led_netdev "wifi1" "wifi1" "maxlinkac1200g:green:wlan_5g" "wlan1" "link"
	ucidef_set_led_default "wps" "wps" "maxlinkac1200g:green:wps" "0"
	;;
esac

board_config_flush

exit 0
