/dts-v1/;

#include "RTL8197F.dtsi"

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
	compatible = "intelbras,gf1200", "realtek,rtl8197f-soc";
	model = "Intelbras GF1200 V1";

	memory {
		device_type = "memory";
		reg = <0x0 0x4000000>;
	};

	chosen {
		bootargs = "console=ttyS0,38400";
	};

	gpio-keys-polled {
		compatible = "gpio-keys-polled";
		#address-cells = <1>;
		#size-cells = <0>;
		poll-interval = <20>;

		reset {
			label = "reset";
			gpios = <&gpio1 4 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};
	};

	gpio-leds {
		compatible = "gpio-leds";

		wan {
			label = "gf1200:blue:wan";
			gpios = <&gpio1 3 GPIO_ACTIVE_LOW>;
		};

		lan {
			label = "gf1200:blue:lan";
			gpios = <&gpio1 1 GPIO_ACTIVE_LOW>;
		};

		wlan {
			label = "gf1200:blue:wlan";
			gpios = <&gpio2 8 GPIO_ACTIVE_LOW>;
		};
	};
};

&gpio0 {
	status = "okay";
};

&gpio1 {
	status = "okay";
};

&gpio2 {
	status = "okay";
};

&spi0 {
	status = "okay";

	m25p80@0 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <29000000>;

		partition@0 {
			label = "boot";
			reg = <0x0 0x30000>;
			read-only;
		};

		partition@30000 {
			label = "firmware";
			reg = <0x30000 0x7B0000>;
		};

		partition@7E0000 {
			label = "config";
			reg = <0x7E0000 0x20000>;
			read-only;
		};
	};
};
